INSERT INTO herp WITH { "Hello" : "World" };
UPDATE p_name WITH { "A": "B" } WHERE { "C": "D" };
UPDATE herp WITH { "Hello" : "Goodbye" } WHERE { "Hello" : "Moto" } LIMIT 1;
DELETE * FROM p_name WHERE { "A": "B" } LIMIT 2;
