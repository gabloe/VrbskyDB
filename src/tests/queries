create project A;
create project A with documents (A,B,C);
create   document A.B;
delete   project A;
delete document   A.B;
select A,B, C   from A.B;
select avg(A),   min(B),max(C), d from A.*;
select * from   A.B;
remove  from A.B where key="A";
append to A.B {"A": [1,2,3,4,5};
show    projects ;
show documents in A;
